local Branch = "main"
local NotificationTime = 5
local IsLocal = true

-- Replace %s placeholders with actual values
local Source = ("%sLoader.lua"):format("https://raw.githubusercontent.com/Remember008862/Parvus/main/")
local BranchStr = Branch
local NotificationTimeStr = NotificationTime
local IsLocalStr = tostring(IsLocal)

-- Load
loadstring(
    IsLocal and readfile("Parvus/Loader.lua") or game:HttpGet(Source),
    "Loader"
)(BranchStr, NotificationTimeStr, IsLocalStr)

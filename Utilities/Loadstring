local Branch = "main"          -- example branch
local NotificationTime = 5      -- example
local IsLocal = true            -- example

-- make sure Parvus table exists exactly like the loader expects
getgenv().Parvus = getgenv().Parvus or {}
getgenv().Parvus.Source = "https://raw.githubusercontent.com/Remember008862/Parvus/main/"

local FileContent
if IsLocal then
    FileContent = readfile("Parvus/Loader.lua")
else
    FileContent = game:HttpGet(getgenv().Parvus.Source.."Loader.lua")
end

if FileContent then
    local LoaderFunc, Err = loadstring(FileContent, "Loader")
    if LoaderFunc then
        LoaderFunc(Branch, NotificationTime, IsLocal)
    else
        warn("loadstring failed:", Err)
    end
else
    warn("File content is nil!")
end
